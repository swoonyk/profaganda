{
  "name": "profaganda-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev": "pnpm --parallel --recursive dev",
    "build": "pnpm --recursive build",
    "start": "pnpm --parallel --recursive start",
    "lint": "pnpm --recursive lint",
    "clean": "pnpm --recursive clean",
    "type-check": "pnpm --recursive type-check",
    "web:dev": "pnpm --filter web dev",
    "api:dev": "pnpm --filter api dev",
    "pipeline:dev": "pnpm --filter pipeline dev",
    "pipeline:ingest": "pnpm --filter pipeline start ingest",
    "pipeline:stats": "pnpm --filter pipeline start stats",
    "db:migrate": "pnpm --filter database migrate",
    "db:setup": "pnpm db:migrate && pnpm pipeline:ingest"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/node": "^22.13.11"
  }
}
